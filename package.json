{
  "name": "@botmock-api/text",
  "version": "0.2.1",
  "description": "process text from the botmock api",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublishOnly": "tsc"
  },
  "keywords": [
    "botmock",
    "typescript",
    "text"
  ],
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "diagnostics": false
      }
    }
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 80
  },
  "bugs": "https://github.com/Botmock/text/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Botmock/text.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "typescript": "3.6.3"
  }
}
